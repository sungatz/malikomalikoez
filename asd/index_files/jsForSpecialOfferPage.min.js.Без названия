function loadComplectPackagesContent(){const n=$("#gift-complects-container"),r=n.data("url");if(n.length&&packageTypeId&&r){const t={typeId:packageTypeId,page:undefined,categories:undefined,sortType:undefined},i=JSON.parse(sessionStorage.getItem(selectedStocksPackagesFiltersKey));if(i)for(const n in i)t.hasOwnProperty(n)&&(t[n]=i[n]);n.addClass("disabled");$.ajax({url:r,type:"POST",data:t,success:function(t){t.errorMessage?(n.html(t.errorMessage),$(".no-packages-found").removeClass("d-none")):t.blockContent&&(n.html(t.blockContent),initShowMorePackagesButtons(t),initCategoriesChange())},error:function(){},complete:()=>{n.removeClass("disabled")}})}}function initShowMorePackagesButtons(n){if(n){let t=undefined;if(n.isGiftsPackages?(packagesContentContainer=$("#gift-packages"),t=packagesContentContainer.parent().find("#show-more-gifts-button")):n.isComplectsPackages&&(packagesContentContainer=$("#complects-packages"),t=packagesContentContainer.parent().find("#show-more-complects-button")),packagesContentContainer&&t)t.on("click",function(){loadStocksPackages({callerButton:$(this),contentContainer:packagesContentContainer})})}}function initCategoriesChange(){$(".filters__checkbox .filters__checkbox-inner").on("click",function(n){n.preventDefault();let i=$(this).find("input:checkbox"),t="checked";i.attr(t)?i.removeAttr(t):i.attr(t,t)});$("#apply-complects-filters").on("click",function(){loadStocksPackages({callerButton:$(this),contentContainer:packagesContentContainer,updateGoodsByFilters:!0})});$('#stocks-packages-sorting input[name="stocks-packages-sorting"]').on("change",function(){loadStocksPackages({contentContainer:packagesContentContainer,updateGoodsByFilters:!0});$(this).closest(".popup-js").fadeOut(300);bodyStocksComplects.removeClass("popup__body")});$(".stocks-filters__menu-show-js").on("click",function(n){n.preventDefault();$(".stocks-filters__menu-js").addClass("active");bodyStocksComplects.addClass("filters__menu-show")});$(".stocks-filters__menu-close-js").on("click",function(n){n.preventDefault();$(".stocks-filters__menu-js").removeClass("active");bodyStocksComplects.removeClass("filters__menu-show")})}function getSelectedCategoriesFilterString(){let n=[];const t=$(".filters__checkbox-inner-checkbox input").filter(":checked");for(let i=0;i<t.length;i++)n.push($(t[i]).data("unique-name"));return n.join("-")}function loadStocksPackages(n){const{i=undefined,t=undefined,r=!1}=n;if(t||t.length){const u=t.parents("#packages-content").data("url"),f=t.data("next-page"),e=getSelectedCategoriesFilterString(),o=$('#stocks-packages-sorting input[name="stocks-packages-sorting"]:checked').data("sort-by");packageTypeId&&u&&(t.addClass("disabled"),i&&i.addClass("btn-loading"),$.ajax({url:u,type:"POST",data:{typeId:packageTypeId,page:f,categories:e,sortType:o,updateGoodsByFilters:r},success:function(n){const u=$(".no-packages-found"),i=t.siblings(".gift-complects-load-button-container");n.packagesByFiltersNotFound?(htmlBody.animate({scrollTop:0},"slow"),t.html(""),i.addClass("d-none"),u.removeClass("d-none")):(r&&(window.sessionStorage.setItem(selectedStocksPackagesFiltersKey,JSON.stringify({page:f,categories:e,sortType:o})),htmlBody.animate({scrollTop:0},"slow"),t.html("")),u.addClass("d-none"),t.append(n.blockContent),n.nextPage&&n.nextPage<=0?i.addClass("d-none"):(i.removeClass("d-none"),t.data("next-page",n.nextPage)))},error:function(){},complete:()=>{t.removeClass("disabled"),i&&i.removeClass("btn-loading")}}))}}function initCardTooltip(){const n=$document.find(".tooltip-labels-js");for(let t=0;t<n.length;t++){const i=$(n[t]);if(!i.hasClass("labelOn")){if(i)i.tooltip({classes:{"ui-tooltip":"ui-tooltip ui-tooltip-label"},position:{my:"center top+5",at:"center bottom+5",collision:"flipfit"},content:function(){return $(this).prop("title")}}).on("mouseout",function(n){$(this).hasClass("clicked")&&n.stopImmediatePropagation()});i.addClass("labelOn")}}}function initSpecialOfferData(){if(filtersBlock){const n=filtersBlock.data("loadurl");if(n){if(productsBlock.length){const n=productsBlock.data("loadurl");productsBlock.addClass("loading");n&&$.ajax({url:n,type:"GET",success:function(n){n&&(n.productsNotFound&&packagesBlock.parents("#offer-goods-container").hide(),productsBlock.html(n.products),paginationBlock.html(n.paginator),initSwipers(),initCardTooltip())},error:function(n,t,i){console.log(n.status);console.log(i)},complete:function(){productsBlock.removeClass("loading")}})}if(packagesBlock.length){const n=packagesBlock.data("loadurl");packagesBlock.addClass("loading");n&&$.ajax({url:n,type:"GET",success:function(n){n&&(n.packagesNotFound&&packagesBlock.parents("#offer-goods-container").hide(),packagesBlock.html(n.packages),paginationBlock.html(n.paginator),initPackageSwipers(),setUpPackageButtons())},error:function(n,t,i){console.log(n.status);console.log(i)},complete:function(){packagesBlock.removeClass("loading")}})}$.ajax({url:n,type:"GET",success:function(n){n&&(filtersBlock.html(n.filters),topBlock.html(n.topBlock))},error:function(n,t,i){console.log(n.status);console.log(i)}})}}}function loadFilters(n){n&&(specialOfferBody.hasClass("filters__menu-show")?(specialOfferBody.addClass("loading"),filtersBlock.find(".filters__menu-close-js").click()):filtersBlock.addClass("disabled"),$.ajax({url:n,type:"GET",success:function(n){n&&(filtersBlock.html(n.filters),topBlock.html(n.topBlock))},error:function(n,t,i){console.log(n.status);console.log(i)},complete:function(){filtersBlock.removeClass("disabled")}}))}function loadGoods(n){n&&productsBlock.length&&(productsBlock.addClass("disabled"),$.ajax({url:n,type:"GET",success:function(n){n&&(productsBlock.html(n.products),paginationBlock.html(n.paginator),initSwipers(),changeBuyBtns(),initCardTooltip())},error:function(n,t,i){console.log(n.status);console.log(i)},complete:function(){productsBlock.removeClass("disabled");specialOfferBody.removeClass("loading")}}))}function loadPackages(n){n&&packagesBlock.length&&(packagesBlock.addClass("disabled"),$.ajax({url:n,type:"GET",success:function(n){n&&(packagesBlock.html(n.packages),paginationBlock.html(n.paginator),initPackageSwipers(),setUpPackageButtons())},error:function(n,t,i){console.log(n.status);console.log(i)},complete:function(){packagesBlock.removeClass("disabled");specialOfferBody.removeClass("loading")}}))}function initSpecialOfferEventListeners(){filtersBlock.on("click",".filters__subcategories-link",function(){const n=$(this);loadGoods(n.data("goodsurl"));loadFilters(n.data("filtersurl"));loadPackages(n.data("packagesurl"))});filtersBlock.on("change",'#filters__sorting input[name="sorting"]',function(){const n=$(this);specialOfferBody.addClass("loading");loadGoods(n.data("goodsurl"));loadFilters(n.data("filtersurl"));n.closest(".popup-js").fadeOut(300);specialOfferBody.removeClass("popup__body")});topBlock.on("click",".filters__top-items a:not(.active)",function(){const n=$(this);loadGoods(n.data("goodsurl"));loadFilters(n.data("filtersurl"))});topBlock.on("click",".products__view-js a",function(){$(this).hasClass("active")||($(this).closest(".products__view-js").find("a.active").removeClass("active"),$(this).addClass("active"),$(this).hasClass("products__items-lines-js")?$(".products__items-js").addClass("products__items-lines"):$(".products__items-js").removeClass("products__items-lines"))});paginationBlock.on("click","li a",function(n){n.preventDefault();const t=$(this);if(!t.hasClass("active")){loadGoods(t.attr("data-paginatorurl"));loadPackages(t.attr("data-paginatorurl"));const i=specialOfferBody.find(".title__xmedium.title__block"),r=specialOfferBody.find(".header__main").height();$("html, body").animate({scrollTop:i.offset().top-20-r},1250)}})}function loadMoreProducts(n){const t=$(n),r=t.data("url"),e=t.parents(".product__item");if(r){let u=t.attr("data-selected-pages"),f=parseInt(t.attr("data-next-page")),i=[];u&&(i=u.split(",").map(Number));f&&i.push(f);productsBlock.addClass("disabled");$.ajax({url:r,type:"GET",dataType:"json",traditional:!0,data:{selectedPages:i},success:function(n){n&&(e.remove(),productsBlock.append(n.products),paginationBlock.html(n.paginator),initSwipers(),changeBuyBtns(),initCardTooltip())},error:function(n,t,i){console.log(n.status);console.log(i)},complete:function(){productsBlock.removeClass("disabled")}})}}function initPackageSwipers(){if(extraDataSwiper=new Swiper(".swiper-kit:not(.swiper-initialized)",{navigation:{nextEl:".product-swiper-button-next-kit",prevEl:".product-swiper-button-prev-kit"}}),extraDataSwiper&&extraDataSwiper.length>1)for(let n=0;n<extraDataSwiper.length;n++){const t=$(extraDataSwiper[n]);t.on("slideChange",function(n){const t=$(n.slides[n.activeIndex]),i=t.closest(".product__kit-wrapper"),r=t.data("buy-btns-id");i.find(".product__kit-item-total").hide();i.find(`#${r}`).show()})}else if(extraDataSwiper&&extraDataSwiper.length)extraDataSwiper.on("slideChange",function(n){const t=$(n.slides[n.activeIndex]),i=t.closest(".product__kit-wrapper"),r=t.data("buy-btns-id");i.find(".product__kit-item-total").hide();i.find(`#${r}`).show()})}function setUpPackageButtons(){const n=packagesBlock.find(".product__kit-wrapper");for(let t=0;t<n.length;t++){const r=$(n[t]),i=r.find(".product__kit-item-total");i.hide();i.first().show()}changeBuyBtns()}function toggleClass(n){const t=$(n.currentTarget).data("activeClass");if(t&&t.length){const i=$(`[data-active-class="${t}"]`);for(let n=0;n<i.length;n++)$(i[n]).removeClass(t);$(n.currentTarget).addClass(t)}}function hideBlock(n){const t=$(n.currentTarget).data("hide");if(t){const i=$(t);if(i.length){const t=$(n.currentTarget).data("animation");t?i.hide(t):i.hide();i.prop("hidden",!0)}}}function shwoBlock(n){const t=$(n.currentTarget).data("show"),i=700;if(t){const r=$(t);let u=$(".qna-blocks-desktop");if(r.length){const t=$(n.currentTarget).data("animation");t?r.show(t):r.show();r.prop("hidden",!1);toggleClass(n);$("html, body").animate({scrollTop:u.offset().top-i},"slow")}}}function toggleBlock(n){const t=$(n.currentTarget).data("toggle");if(t){const i=$(t);if(i.length){const t=$(n.currentTarget).data("animation");t?i.toggle(t):i.toggle();for(let n=0;n<i.length;n++)$(i[n]).prop("hidden",!$(i[n]).prop("hidden"));toggleClass(n);playScrollAnimation($(n.currentTarget))}}}function playScrollAnimation(n){let t=n.hasClass("cab")&&!n.hasClass("s_block"),i=n.hasClass("mobile");(t||i)&&$("html, body").animate({scrollTop:n.offset().top-50},"slow")}function createCitySelect(n,t){n.html("");data={defaultValue:n.data("default-value")};$.ajax({type:"GET",url:"/Home/GetCitiesDropDownList",data:data,success:function(i){n.html(i);n=$(t);let r=n.data("default-value-disabled");r&&n.find("option").first().prop("disabled",!0);$(".cities-drop-down-list.cab").length&&$(".cities-drop-down-list select").change(function(){const t=$(".cities-drop-down-list"),n=t.data("animation"),i=`${t.data("show")}${$(this).val()}`,r=t.data("hide");n?($(r).hide(n),$(i).show(n)):($(r).hide(n),$(i).show(n))})}})}function customFormsSetup(){const n=document.getElementsByClassName("сustom-form");if(!(n.length<=0))for(i=0;i<n.length;i++){let t=n[i],r=t.querySelectorAll("input[data-mask]");for(let n=0;n<r.length;n++){let t=r[n].getAttribute("data-mask");$(r[n]).inputmask(t,{autoUnmask:!0})}t.addEventListener("submit",function(n){n.preventDefault();const i=new XMLHttpRequest,e=t.getAttribute("action"),o=t.getAttribute("method").toUpperCase(),r=document.getElementById(t.getAttribute("data-result"));let u="",f=t.getElementsByTagName("input");for(j=0;j<f.length;j++){let n=f[j];if(n.getAttribute("type")=="submit")continue;else u+=j==0?`${n.getAttribute("name")}=${n.value}`:`&${n.getAttribute("name")}=${n.value}`}i.open(o,e,!0);i.setRequestHeader("Content-type","application/x-www-form-urlencoded");i.addEventListener("readystatechange",()=>{if(i.readyState===4&&i.status===200){let n=JSON.parse(i.responseText);r==null?$.fancybox.open(`<div class="message">${n.message}</div>`):r.innerHTML=n.message}});i.send(u);r.innerHTML="..."})}}function tryGetPreorderForm(){const n=$(".preorder-form-container");if(n.length){const t=n.data("loadurl"),i=n.data("formid");$.ajax({type:"POST",url:t,data:{formId:i},success:function(t){applyPreorderForm(n,t);initPreorderEventListeners(n)}})}}function applyPreorderForm(n,t){n.html(t);initGallerySwipers()}function initGallerySwipers(){const n=new Swiper(".swiper-gallery-thumbs",{loop:!1,slidesPerView:4,spaceBetween:10,direction:"vertical",allowTouchMove:!0,navigation:{nextEl:".gallery-thumbs__nav-next",prevEl:".gallery-thumbs__nav-prev"}});new Swiper(".swiper-gallery-main",{loop:!1,pagination:{el:".swiper-pagination",clickable:!0},navigation:{nextEl:".swiper-button-next",prevEl:".swiper-button-prev"},thumbs:{swiper:n}})}function initPreorderEventListeners(n){n.on("click",".preorder-btn",function(){$(this).addClass("btn-loading")});n.on("click",".prop-item:not(.active)",function(){const t=$(".model-selector").data("selecteditem"),i=n.data("loadurl"),r=n.data("formid"),u=$(this).data("slug"),f=n.find(".product__main-info-block").addClass("loading");$.ajax({type:"POST",url:i,data:{formId:r,slug:u,itemId:t},success:function(t){applyPreorderForm(n,t)},complete:function(){f.removeClass("loading")}})});n.on("click",".model-item:not(.active)",function(){const t=$(this).data("itemid"),i=n.data("loadurl"),r=n.data("formid"),u=n.find(".product__main-info-block").addClass("loading");$.ajax({type:"POST",url:i,data:{formId:r,itemId:t},success:function(t){applyPreorderForm(n,t)},complete:function(){u.removeClass("loading")}})});n.on("click",".payment-option:not(.active)",function(){n.find(".full-payment-button").toggle();n.find(".pre-payment-button").toggle()})}function initInputLabels(){promoInputForm.on("blur",".form__label-input-js .form__input",function(n){n.preventDefault();var t=$(this).val();t!=""?$(this).addClass("form__input-fill"):$(this).removeClass("form__input-fill")});promoInputForm.on("click",".form__label-input-js",function(){$(this).children(".form__input").focus()})}function initPlaceholderUp(){promoInputForm.on("mouseover","input",function(){$(this).addClass("form__input-fill")})}function initPlaceholderDown(){promoInputForm.on("mouseleave","input",function(){$(this).val().length===0&&$(this).removeClass("form__input-fill")})}function initPromoPopup(n){$.fancybox.open(n);const u=$("#promo-block");u.find("#promo-input-surname").change(function(){validateName($(this))});const i=u.find("#promo-input-name");i.on("keyup change",function(){validateName($(this))});if(i.val()){const n=i.val();if(i.val(n),validateName(i)){i.attr("readonly",!0);i.on("DOMSubtreeModified",function(){i.attr("readonly")||i.attr("readonly",!0)})}}const r=u.find("#promo-input-email");r.on("keyup change",function(){validateEmail($(this))});if(r.val()){const n=r.val();if(r.val(n),validateEmail(r)){r.attr("readonly",!0);r.on("DOMSubtreeModified",function(){r.attr("readonly")||r.attr("readonly",!0)})}}const f=$body.attr("data-locale"),t=u.find("#promo-input-phone");if(t.val()){const n=t.val();if(f==="kg")t.val(n.replace(/(\d{3})(\d{3})(\d{3})(\d{3})/g,"+$1 ($2) $3-$4"));else t.val(n.replace(/(\d?)(\d{3})(\d{3})(\d{2})(\d{2})/g,"+$1 ($2) $3-$4-$5"));if(validatePhone(t)){t.attr("readonly",!0);t.on("DOMSubtreeModified",function(){t.attr("readonly")||t.attr("readonly",!0)})}}else f==="kg"?(t.inputmask("+\\9\\96 (999) 999-999"),t.attr("placeholder","+996 (___) ___-___")):(t.inputmask("+7 (999) 999-99-99"),t.attr("placeholder","+7 (___) ___-__-__"));t.change(function(){validatePhone($(this))});u.find("#promo-input-city").change(function(){validateCity($(this))});u.find("#promo-input-submit-button").click(function(n){n.preventDefault();n.stopPropagation();const i=$(this);if(!isValidForm(i.parents("#promo-input-form"))){console.log("Invalid form");return}const t=new FormData,r=i.parents("#promo-block").find("input, select").not('input[type="submit"]');for(let n=0;n<r.length;++n){const i=$(r[n]);i.attr("type")!=="radio"?t.append(i.attr("name"),i.val()):i.is(":checked")&&t.append(i.attr("name"),i.val())}const f=i.attr("data-send-url");f?generatePromoResult(f,t):$.ajax({type:"POST",url:"/promo/send-sulpak-promo",data:t,processData:!1,contentType:!1,success:function(n){$.fancybox.close();initPromoResultPopup(u,!0,n)},error:function(){$.fancybox.close();initPromoResultPopup(u,!1)}})})}function initPromoResultPopup(n,t,i=true){const r=n.find("#promo-result");t?i?r.text(n.find("#promo-result-success-text").text()):r.text(n.find("#promo-result-duplicate-text").text()):r.text(n.find("#promo-result-error-text").text());$.fancybox.open(r)}function isValidForm(n){return validateName(n.find("#promo-input-name")),validateEmail(n.find("#promo-input-email")),validatePhone(n.find("#promo-input-phone")),n.find(".invalid-input").length===0}function validateName(n){const t=n.val().trim(),i=n.next().next();if(isNullOrEmpty(t)||!/^[a-zа-яё-]+\s?[a-zа-яё-]+$/i.test(t))n.addClass("invalid-input"),i.css("display","block");else return n.removeClass("invalid-input"),i.css("display","none"),!0;return!1}function validateEmail(n){const t=n.val().trim(),i=n.next().next();if(isNullOrEmpty(t)||!/^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/.test(t))n.addClass("invalid-input"),i.css("display","block");else return n.removeClass("invalid-input"),i.css("display","none"),!0;return!1}function validatePhone(n){const t=n.val(),i=n.next().next();if(isNullOrEmpty(t)||t.length-t.replace(/[^0-9]/g,"").length>7)n.addClass("invalid-input"),i.css("display","block");else return n.removeClass("invalid-input"),i.css("display","none"),!0;return!1}function validateCity(n){const t=n.val();n.removeClass("invalid-input");isNullOrEmpty(t)&&n.addClass("invalid-input")}function isNullOrEmpty(n){return n==null||n.length===0}function generatePromoResult(n,t){const i=$("#promo-block");$.ajax({type:"POST",url:n,data:t,processData:!1,contentType:!1,success:function(n){$.fancybox.close();initPromoResultResultPopup(i,!0,n.success,n.message)},error:function(){$.fancybox.close();initPromoResultResultPopup(i,!1)}})}function initPromoResultResultPopup(n,t,i=true,r=null){const u=n.find("#promo-result");t?i?u.html(r):u.text(n.find("#promo-result-duplicate-text").text()):u.text(n.find("#promo-result-error-text").text());$.fancybox.open(u)}function initPromoDiscount10Popup(n){$.fancybox.open(n);const t=$("#promo-discount10-block");t.find("#promo-input-name").change(function(){validateName($(this))});t.find("#promo-input-email").change(function(){validateEmail($(this))});const r=$("body").attr("data-locale"),i=t.find("#promo-input-phone");r==="kg"?(i.inputmask("+\\9\\96 (999) 999-999"),i.attr("placeholder","+996 (___) ___-___")):(i.inputmask("+7 (999) 999-99-99"),i.attr("placeholder","+7 (___) ___-__-__"));i.change(function(){validatePhone($(this))});t.find("#promo-input-city").change(function(){validateCity($(this))});t.find("#promo-input-submit-button").click(function(n){n.preventDefault();n.stopPropagation();const i=$(this);if(isValidForm(i.parents("#promo-input-form"))){const r=new FormData,u=i.parents("#promo-discount10-block").find("input, select").not('input[type="submit"]');for(let n=0;n<u.length;++n){const t=$(u[n]);t.attr("type")!=="radio"?r.append(t.attr("name"),t.val()):t.is(":checked")&&r.append(t.attr("name"),t.val())}addButtonLoader(i);$.ajax({type:"POST",url:"/promo/send-discount10",data:r,processData:!1,contentType:!1,success:function(n){$.fancybox.close();initDiscount10ResultPopup(t,!0,n.success,n.message)},error:function(){$.fancybox.close();initDiscount10ResultPopup(t,!1)},complete:function(){removeButtonLoader(i)}})}})}function initDiscount10ResultPopup(n,t,i=true,r=null){const u=n.find("#promo-discount10-result");t?i?u.html(r):u.text(n.find("#promo-result-duplicate-text").text()):u.text(n.find("#promo-result-error-text").text());$.fancybox.open(u)}const packageTypeId=$(".all-packages").data("type"),selectedStocksPackagesFiltersKey="selected-stocks-packages-filters",htmlBody=$("html, body"),bodyStocksComplects=$document.find("body");let packagesContentContainer;const specialOfferBody=$("body"),filtersBlock=specialOfferBody.find("#filters-block"),topBlock=specialOfferBody.find("#top-block"),productsBlock=specialOfferBody.find("#products"),packagesBlock=specialOfferBody.find("#packages"),paginationBlock=specialOfferBody.find("#pagination-block");let extraDataSwiper;$document.ready(function(){initSpecialOfferData();initSpecialOfferEventListeners();loadComplectPackagesContent()});$(document).ready(function(){const n=$(".custom-action-button, .cab"),t=".cities-drop-down-list",i=$(t);if(n.length)for(let t=0;t<n.length;t++){const i=$(n[t]),r=i.data("event");if(r){if(i.data("hide"))i.on(r,hideBlock);if(i.data("show"))i.on(r,shwoBlock);if(i.data("toggle"))i.on(r,toggleBlock)}}i.length&&createCitySelect(i,t);customFormsSetup()});$document.ready(function(){tryGetPreorderForm()});const promoInputForm=$("#promo-input-form");$(document).on("click",".sulpak-promo-link",function(){const n=$(this),t=n.attr("data-url");n.addClass("btn-loading");$.ajax({type:"POST",url:t,data:{title:n.attr("data-title"),token:n.attr("data-token"),products:n.attr("data-products"),successText:n.attr("data-success-text"),duplicateText:n.attr("data-duplicate-text"),errorText:n.attr("data-error-text"),instagram:n.attr("data-instagram")=="true"||n.attr("data-instagram")=="True",instagramName:n.attr("data-instagram-name"),sendUrl:n.attr("data-send-url")},success:function(n){initPromoPopup(n);initInputLabels();initPlaceholderUp();initPlaceholderDown()},error:function(n){console.error(n)},complete:function(){n.removeClass("btn-loading")}})});$(document).on("click",".copy-btn",function(){const n=$(this).data("code"),t=$(this).data("message");navigator.clipboard&&navigator.clipboard.writeText(n).then(function(){$.fancybox.open(t)})});$(document).on("click","a.promo-discount10-link",function(){const n=$(this),t=n.attr("data-url");addButtonLoader